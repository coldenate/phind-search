{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "phind-search",
  "title": "Phind Search",
  "description": "Search Phind - an AI powered search engine - with speed and agility by going directly through Raycast! ",
  "icon": "command-icon.png",
  "author": "colderinit",
  "categories": [
    "Web",
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Phind",
      "description": "Queries Phind Search with autosuggestions enabled. ",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}